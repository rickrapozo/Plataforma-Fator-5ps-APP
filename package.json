{
  "name": "essential-factor-5p-platform",
  "version": "1.0.0",
  "description": "Plataforma de transformação pessoal baseada no método 5Ps",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint . --ext ts,tsx,js,jsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx,js,jsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "db:migrate": "node database/migrate.cjs migrate",
    "db:seed": "node database/migrate.cjs seed",
    "db:reset": "node database/migrate.cjs reset",
    "cleanup": "node scripts/cleanup.js",
    "quality": "npm run lint:fix && npm run format && npm run test"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@stripe/stripe-js": "^7.9.0",
    "@supabase/supabase-js": "^2.57.0",
    "@types/crypto-js": "^4.2.2",
    "@types/ws": "^8.18.1",
    "axios": "^1.5.0",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "date-fns": "^2.30.0",
    "dexie": "^3.2.4",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "framer-motion": "^10.16.0",
    "js-cookie": "^3.0.5",
    "jspdf": "^3.0.2",
    "lucide-react": "^0.290.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.47.0",
    "react-hot-toast": "^2.6.0",
    "react-router-dom": "^6.8.0",
    "react-toastify": "^9.1.3",
    "recharts": "^2.8.0",
    "socket.io-client": "^4.8.1",
    "tailwind-merge": "^3.3.1",
    "ws": "^8.18.3",
    "zod": "^3.22.0",
    "zustand": "^4.4.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/js-cookie": "^3.0.4",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.1.0",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.0",
    "babel-jest": "^30.1.2",
    "eslint": "^8.50.0",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-transform-css": "^6.0.3",
    "jest-transform-file": "^1.1.1",
    "jsdom": "^26.1.0",
